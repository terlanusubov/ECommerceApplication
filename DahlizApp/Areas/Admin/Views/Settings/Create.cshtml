@model DahlizApp.Models.Setting

@{
    ViewData["Title"] = "Create";
}
<div class="col-12">
    <h2>Create</h2>
    <h4>Setting</h4>
</div>
<hr />
<div class="col-12">
    <div class="row">
        <div class="col-12">
            <form id="settingAddForm" method="post" asp-controller="Settings" asp-action="Create">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <div class="error_wrapper set_logo"></div>
                    <label asp-for="Logo" class="control-label"></label>
                    <input data-element="true" data-type="text" data-error="set_logo" asp-for="Logo" class="form-control" />
                    <span asp-validation-for="Logo" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <div class="error_wrapper set_phone"></div>
                    <label asp-for="Phone" class="control-label"></label>
                    <input data-element="true" data-type="text" data-error="set_phone" asp-for="Phone" class="form-control" />
                    <span asp-validation-for="Phone" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <div class="error_wrapper set_email"></div>
                    <label asp-for="Email" class="control-label"></label>
                    <input data-element="true" type="text" data-type="text" data-error="set_email" asp-for="Email" class="form-control" />
                    <span asp-validation-for="Email" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <div class="error_wrapper set_description"></div>
                    <label asp-for="Description" class="control-label"></label>
                    <input data-element="true" data-type="text" data-error="set_description" asp-for="Description" class="form-control" />
                    <span asp-validation-for="Description" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <div class="error_wrapper set_face"></div>
                    <label asp-for="Facebook" class="control-label"></label>
                    <input data-element="true" data-type="text" data-error="set_face" asp-for="Facebook" class="form-control" />
                    <span asp-validation-for="Facebook" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <div class="error_wrapper set_twitter"></div>
                    <label asp-for="Twitter" class="control-label"></label>
                    <input data-element="true" data-type="text" data-error="set_twitter" asp-for="Twitter" class="form-control" />
                    <span asp-validation-for="Twitter" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <div class="error_wrapper set_instagram"></div>
                    <label asp-for="Instagram" class="control-label"></label>
                    <input data-element="true" data-type="text" data-error="set_instagram" asp-for="Instagram" class="form-control" />
                    <span asp-validation-for="Instagram" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <button type="submit" class="btn btn-primary" id="btnSubmit" data-parent="#settingAddForm">Add</button>
                </div>
            </form>
        </div>
    </div>
</div>
<div class="col-12 mt-2">
    <a asp-action="Index">Back to List</a>
</div>

